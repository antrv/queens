{
    "files.exclude": {
        "**/.git": true,
        "**/.bin": true,
        "**/.tmp": true
    },
    "editor.lineNumbers": "on",
    "editor.fontSize": 15,
    "C_Cpp.intelliSenseEngine": "Default",
    "C_Cpp.autocomplete": "Default",
    "C_Cpp.intelliSenseEngineFallback": "Disabled",
    "[cpp]": {
        "editor.formatOnPaste": true,
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.insertSpaces": true,
        "editor.autoIndent": true,
        "editor.tabSize": 4,
        "editor.renderIndentGuides": true,
        "editor.detectIndentation": false
    },
    "files.associations": {
        "ostream": "cpp",
        "type_traits": "cpp",
        "utility": "cpp",
        "xmemory0": "cpp",
        "xtr1common": "cpp",
        "xutility": "cpp",
        "cstddef": "cpp",
        "algorithm": "cpp",
        "stdexcept": "cpp",
        "string": "cpp",
        "cmath": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cstring": "cpp",
        "cwchar": "cpp",
        "exception": "cpp",
        "initializer_list": "cpp",
        "ios": "cpp",
        "iosfwd": "cpp",
        "istream": "cpp",
        "limits": "cpp",
        "memory": "cpp",
        "new": "cpp",
        "optional": "cpp",
        "streambuf": "cpp",
        "system_error": "cpp",
        "tuple": "cpp",
        "typeinfo": "cpp",
        "xfacet": "cpp",
        "xiosbase": "cpp",
        "xlocale": "cpp",
        "xlocinfo": "cpp",
        "xlocnum": "cpp",
        "xmemory": "cpp",
        "xstddef": "cpp",
        "xstring": "cpp",
        "vector": "cpp",
        "stack": "cpp",
        "functional": "cpp",
        "array": "cpp",
        "ctime": "cpp",
        "deque": "cpp",
        "iostream": "cpp",
        "iterator": "cpp",
        "list": "cpp",
        "unordered_map": "cpp",
        "xhash": "cpp"
    }
}